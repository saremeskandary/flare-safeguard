{
  "name": "@se-2/nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "check-types": "tsc --noEmit --incremental",
    "dev": "next dev",
    "format": "prettier --write . '!(node_modules|.next|contracts)/**/*'",
    "ipfs": "NEXT_PUBLIC_IPFS_BUILD=true yarn build && yarn bgipfs upload config init -u https://upload.bgipfs.com && CID=$(yarn bgipfs upload out | grep -o 'CID: [^ ]*' | cut -d' ' -f2) && [ ! -z \"$CID\" ] && echo 'üöÄ Upload complete! Your site is now available at: https://community.bgipfs.com/ipfs/'$CID || echo '‚ùå Upload failed'",
    "lint": "next lint",
    "lint:errors": "next lint --quiet",
    "serve": "next start",
    "start": "next dev",
    "vercel": "vercel --build-env YARN_ENABLE_IMMUTABLE_INSTALLS=false --build-env ENABLE_EXPERIMENTAL_COREPACK=1 --build-env VERCEL_TELEMETRY_DISABLED=1",
    "vercel:login": "vercel login",
    "vercel:yolo": "vercel --build-env YARN_ENABLE_IMMUTABLE_INSTALLS=false --build-env ENABLE_EXPERIMENTAL_COREPACK=1 --build-env NEXT_PUBLIC_IGNORE_BUILD_ERROR=true --build-env VERCEL_TELEMETRY_DISABLED=1",
    "seed:claims": "ts-node -r tsconfig-paths/register scripts/seed-claims.ts",
    "seed:insurance": "ts-node -r tsconfig-paths/register scripts/seed-insurance-options.ts",
    "seed:policies": "ts-node -r tsconfig-paths/register scripts/seed-policies.ts",
    "remove:insurance": "ts-node -r tsconfig-paths/register scripts/remove-insurance-options.ts",
    "remove:claims": "ts-node -r tsconfig-paths/register scripts/remove-claims.ts",
    "remove:policies": "ts-node -r tsconfig-paths/register scripts/remove-policies.ts",
    "remove:all": "ts-node -r tsconfig-paths/register scripts/remove-all-seed-data.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@helia/dag-cbor": "^4.0.3",
    "@helia/dag-json": "^4.0.3",
    "@helia/http": "^2.0.5",
    "@helia/json": "^4.0.3",
    "@heroicons/react": "~2.1.5",
    "@rainbow-me/rainbowkit": "2.2.3",
    "@tanstack/react-query": "~5.59.15",
    "@uniswap/sdk-core": "~5.8.2",
    "@uniswap/v2-sdk": "~4.6.1",
    "blo": "~1.2.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "burner-connector": "0.0.12",
    "critters": "^0.0.25",
    "crypto-browserify": "^3.12.1",
    "daisyui": "4.12.10",
    "framer-motion": "^12.8.2",
    "helia": "^5.3.0",
    "https-browserify": "^1.0.0",
    "kubo-rpc-client": "~5.0.2",
    "mongodb": "^6.15.0",
    "next": "~15.2.5",
    "next-nprogress-bar": "~2.3.13",
    "next-themes": "~0.3.0",
    "node-fetch": "2",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "qrcode.react": "~4.0.1",
    "react": "~19.0.0",
    "react-copy-to-clipboard": "~5.1.0",
    "react-dom": "~19.0.0",
    "react-hot-toast": "^2.5.2",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "usehooks-ts": "~3.1.0",
    "viem": "2.23.0",
    "wagmi": "2.14.11",
    "zustand": "~5.0.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "~4.3.0",
    "@types/node": "~18.19.50",
    "@types/path-browserify": "^1",
    "@types/react": "~19.0.7",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "~5.40.0",
    "abitype": "1.0.6",
    "autoprefixer": "~10.4.20",
    "bgipfs": "~0.0.12",
    "eslint": "~9.23.0",
    "eslint-config-next": "~15.2.3",
    "eslint-config-prettier": "~10.1.1",
    "eslint-plugin-prettier": "~5.2.4",
    "postcss": "~8.4.45",
    "prettier": "~3.5.3",
    "tailwindcss": "~3.4.11",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "type-fest": "~4.26.1",
    "typescript": "~5.8.2",
    "vercel": "~39.1.3"
  },
  "packageManager": "yarn@3.2.3"
}
